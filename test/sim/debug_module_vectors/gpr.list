# data0 is at 0x04
# command is at 0x17
# command for reg write is 0x00231000 + reg
# command for reg read  is 0x00221000 + reg

# Turn on dm
w 0x10 1
# Request halt
w 0x10 0x80000001
# Read back halt status
i 30
r 0x11

# write to registers a0, a1
w 0x04 0x1234
w 0x17 0x00231008
w 0x04 0x5678
w 0x17 0x00231009

# Read them back
w 0x17 0x00221008
r 0x04
w 0x17 0x00221009
r 0x04

x
